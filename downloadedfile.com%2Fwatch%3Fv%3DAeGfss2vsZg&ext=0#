<script type="text/javascript">

    $(function()
    {
        $('#log1 .closetitle1, #log1 .closebtn1').on('click', function(e){
            $(this).blur();
            cookie.set('logopen', '0' , 365);
            $('#log1').hide();
        });
        $('#log1 .clear1').on('click', function(e){
            $(this).blur();
            log.clear();
        });
        $('#openlog1').on('click', function(e){
            e.preventDefault();
            $(this).blur();
            var isShown = $('#log1').is(':visible');
            if (!isShown) {
                cookie.set('logopen', '1' , 365);
                $('#log1').show();
            }
            else {
                cookie.set('logopen', '0' , 365);
                $('#log1').hide();
            }
        });

        if (cookie.get('logopen') == '1') {
            $('#log1').show();
        } else {
            $('#log1').hide();
        }
    });


</script>
            
